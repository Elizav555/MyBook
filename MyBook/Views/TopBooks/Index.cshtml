@model List<MyBook.Entities.Book>

<div class="container main_wrap">
    <div class="content_wrapper ">
        <div class="customCarousel">
            <h1 class="main_title_book ms-4 mb-4">Топ книги</h1>
            @await Component.InvokeAsync("BooksCarousel", Model.OrderByDescending(book => book.DownloadsCount).ToList())
        </div>
    </div>
</div>
